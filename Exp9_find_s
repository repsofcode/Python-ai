import csv

def find_s(file):
    with open(file, 'r') as f:
        data = list(csv.reader(f))

    # number of attributes = last column is label
    n = len(data[0]) - 1
    hypothesis = None

    for row in data:
        attrs = row[:n]
        label = row[n].lower()

        if label == 'yes':              # positive example
            if hypothesis is None:
                hypothesis = attrs[:]   # first positive â†’ initialize hypothesis
            else:
                for i in range(n):
                    if hypothesis[i] != attrs[i]:
                        hypothesis[i] = '?'  # generalize
    return hypothesis


# Example
h = find_s("training_data.csv")
print("Hypothesis:", h)


csv
Sunny,Hot,High,Weak,yes
Sunny,Hot,High,Strong,yes
Rainy,Cold,High,Weak,no
Sunny,Hot,High,Weak,yes
